{
  "name": "macos-vm",
  "version": "0.7.0",
  "description": "Instructions and script to help you create a VirtualBox VM running MacOS",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/macos-vm.git"
  },
  "engines": {
    "node": ">=10.21.0",
    "npm": ">=6.8.0"
  },
  "scripts": {
    "prepare": "husky install",
    "updates": "npx ncu",
    "release": "np"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "commitlint-config-gitmoji": "^2.2.6",
    "husky": "^7.0.4"
  },
  "commitlint": {
    "extends": [
      "gitmoji"
    ],
    "rules": {
      "subject-empty": [
        0,
        "never"
      ],
      "type-empty": [
        0,
        "never"
      ]
    }
  }
}
